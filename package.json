{
  "name": "pdp-client",
  "version": "0.0.0",
  "description": "pdp client front-end",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha test/helpers/browser.js test/**/*spec.js --compilers js:babel-core/register --recursive || true",
    "test-watch": "NODE_ENV=test mocha test/helpers/browser.js test/**/*spec.js --compilers js:babel-core/register --recursive --watch || true",
    "start": "NODE_ENV=development webpack-dev-server",
    "build": "NODE_ENV=production webpack --config webpack.prod.config.js",
    "profile": "NODE_ENV=production webpack --config webpack.prod.config.js --profile --json > profile.json",
    "lint": "NODE_ENV=lint webpack-dev-server --config webpack.lint.config.js",
    "elint": "eslint . --ext .js --ext .jsx --cache || true",
    "elinttable": "eslint . --ext .js --ext .jsx --cache --format table || true",
    "elintsummary": "eslint . --ext .js --ext .jsx --cache --format 'node_modules/eslint-friendly-formatter' || true",
    "slint": "stylelint ./app/styles/**/*.css ./app/style.css --syntax scss || true",
    "cover": "NODE_ENV=test babel-node node_modules/.bin/babel-istanbul cover _mocha -- --require test/helpers/browser.js test/**/*.spec.js --reporter dot || true",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "author": "me",
  "license": "ISC",
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.6.1",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-polyfill": "^6.13.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "browser-sync": "^2.14.0",
    "browser-sync-webpack-plugin": "^1.1.2",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-config-google": "^0.6.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-minifier": "^3.0.1",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.4.2",
    "mocha": "^3.0.2",
    "normalize-css": "^2.3.1",
    "postcss-easy-import": "^1.0.1",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylelint": "^7.1.0",
    "stylelint-config-standard": "^12.0.0",
    "stylelint-webpack-plugin": "^0.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "@kadira/storybook": "^2.0.0"
  },
  "directories": {
    "test": "test"
  },
  "keywords": [
    "webpack",
    "reactjs",
    "redux",
    "postcss"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevenyoung/pdp-client.git"
  },
  "bugs": {
    "url": "https://github.com/stevenyoung/pdp-client/issues"
  },
  "homepage": "https://github.com/stevenyoung/pdp-client#readme"
}
